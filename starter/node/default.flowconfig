[ignore]
# Ignore underscore files.
.*/_[^_].*

# Ignore compiled code.
.*/lib/.*

# Ugh
.*node_modules/babel.*
.*node_modules/fbjs.*
.*node_modules/json5.*

[include]

[libs]

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
suppress_type=$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
